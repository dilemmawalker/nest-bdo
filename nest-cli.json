{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "root": "apps/api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "libs/core/users",
      "entryFile": "index",
      "sourceRoot": "libs/core/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "agent": {
      "type": "application",
      "root": "apps/agent",
      "entryFile": "main",
      "sourceRoot": "apps/agent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agent/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "boilerplate": {
      "type": "application",
      "root": "apps/boilerplate",
      "entryFile": "main",
      "sourceRoot": "apps/boilerplate/src",
      "compilerOptions": {
        "tsConfigPath": "apps/boilerplate/tsconfig.app.json"
      }
    }
  }
}